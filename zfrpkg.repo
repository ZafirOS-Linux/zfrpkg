#
# /etc/zfrpkg.repo : scratchpkg repo file
#
# format:
#    <repo directory> <repo url for sync>
#
ports:
  /usr/ports/myrepo
  /usr/ports/core        https://github.com/ZafirOS-Linux/ports/tree/venom4/core
  /usr/ports/main        https://github.com/ZafirOS-Linux/ports/tree/repos/main
  /usr/ports/multilib    https://github.com/ZafirOS-Linux/ports/tree/repos/multilib
